<Window x:Class="No_Mole.Components.InspectionDetailModal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:No_Mole.Components"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStyle="None"
        Title="InspectionDetailModal" Height="415" Width="615">
    <Grid>
        <StackPanel Orientation="Vertical">
            <UniformGrid Rows="1" HorizontalAlignment="Stretch" Margin="50 24">
                <Label HorizontalAlignment="Left">Device Details</Label>
                <Button Content="Close" HorizontalAlignment="Right" Click="Button_Click"></Button>
            </UniformGrid>
        </StackPanel>
    </Grid>
</Window>
